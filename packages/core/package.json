{
  "name": "@prismui/core",
  "version": "0.0.1",
  "description": "Prismui core components with polymorphic support",
  "homepage": "https://prismui.dev/",
  "license": "MIT",
  "author": "Fangjun <fangjun20208@gmail.com>",
  "keywords": [
    "react",
    "ui",
    "components",
    "polymorphic",
    "prismui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/medxaidev/prismui.git",
    "directory": "packages/core"
  },
  "sideEffects": [
    "*.css"
  ],
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc  && node ./scripts/esbuild.mjs",
    "dev": "",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "type-fest": "^5.4.3"
  },
  "devDependencies": {
    "react": "19.2.4",
    "react-dom": "19.2.4"
  }
}
